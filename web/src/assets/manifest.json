{
  "manifest_version": 3,
  "name": "Tool",
  "version": "1.0",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage", 
    "nativeMessaging", 
    "alarms", 
    "notifications",
    "scripting"
],
  "action": {
    "default_popup": "index.html"
  }
}
